{
  "name": "bouncehousekids-server",
  "version": "1.0.0",
  "description": "Backend server for Bounce House Rental System",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:related": "jest --bail --findRelatedTests",
    "lint": "eslint src/**/*.{js,ts} --quiet",
    "lint:fix": "eslint src/**/*.{js,ts} --quiet --fix",
    "seed": "ts-node src/scripts/seed.ts",
    "create-token": "ts-node src/scripts/createAdminToken.ts"
  },
  "keywords": [
    "bounce-house",
    "rental",
    "nodejs",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@types/multer-s3": "^3.0.3",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "bounce-house-rental-system": "file:..",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "stripe": "^12.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.0.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^9.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
